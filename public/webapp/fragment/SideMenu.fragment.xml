<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns="sap.tnt"
	height="100%">
	<SideNavigation 
		visible="{session>/authenticated}"
		expanded="{sideMenu>/expanded}" 
		selectedItem="subItemThree">
		<item>
			<NavigationList>
				<NavigationListItem
					text="Item 1"
					icon="sap-icon://employee">
					<NavigationListItem text="Sub Item 1" />
					<NavigationListItem text="Sub Item 2" />
					<NavigationListItem text="Sub Item 3" id="subItemThree"/>
					<NavigationListItem text="Sub Item 4" />
				</NavigationListItem>
				<NavigationListItem
					text="Item 2"
					icon="sap-icon://building">
					<NavigationListItem text="Sub Item 1" />
					<NavigationListItem text="Sub Item 2" />
					<NavigationListItem text="Sub Item 3" />
					<NavigationListItem text="Sub Item 4" />
				</NavigationListItem>
			</NavigationList>
		</item>
		<fixedItem>
			<NavigationList>
				<NavigationListItem text="Item 1" icon="sap-icon://employee"/>
				<NavigationListItem text="Item 2" icon="sap-icon://building"/>
				<NavigationListItem text="Item 3" icon="sap-icon://card"/>
			</NavigationList>
		</fixedItem>
	</SideNavigation>
</core:FragmentDefinition>
