<mvc:View
	controllerName="smartie.loteca.website.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	displayBlock="true"
	xmlns:tnt="sap.tnt"
	xmlns="sap.m"
	>
	<App id="root">
		<pages>
			<tnt:ToolPage sideExpanded="{sideMenu>/expanded}" id="page">
				<tnt:header>
					<core:Fragment fragmentName="smartie.loteca.website.fragment.Menu" type="XML" />
				</tnt:header>
				<tnt:sideContent>
					<core:Fragment fragmentName="smartie.loteca.website.fragment.SideMenu" type="XML" />
				</tnt:sideContent>
				<tnt:mainContents>
					<NavContainer id="main">
						<pages>
						</pages>
					</NavContainer>
				</tnt:mainContents>
			</tnt:ToolPage>
		</pages>
	</App>
</mvc:View>